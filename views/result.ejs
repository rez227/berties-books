  </section>
  <h1>Search Results</h1>
  <% if (results.length> 0) { %>
    <ul>
      <% results.forEach(result=> { %>
        <li>
          <strong>occasion:</strong>
          <%= result.occasion %><br>
            <strong>colour:</strong>
            <%= result.colour %>
              <strong>budget:</strong>
              <%= result.budget %>

        </li>
        <% }); %>
    </ul>
    <% } else { %>
      <p>No results found.</p>
      <% } %>
</body>

</html>